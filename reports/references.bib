}

@book{sarna_regulatory_2010,
	title = {Regulatory {Mechanisms}},
	url = {http://www.ncbi.nlm.nih.gov/books/NBK53475/},
	abstract = {The generation of gut contractions and their organization into spatiotemporal patterns that result in propagating and nonpropagating contractions is autonomous. Specifically, input from the extrinsic nerves or hormones is not required to generate or organize them. The smooth muscle cells and the enteric neurons are the core cell types that together regulate motility function autonomously (Figure 9). However, input from external sources (sympathetic and parasympathetic neurons; paracrine, endocrine, and exocrine hormones; inflammatory mediators or stress mediators) may influence the myogenic and enteric neuronal regulation to modify the characteristics of gut contractions in response to changes in the external environment. The autonomic nerves do not directly innervate the smooth muscle cells. Instead, they modulate the activity of the enteric nervous system, which, in turn, affects the overall motility function.},
