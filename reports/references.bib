	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/9780470479216.corpsy0524},
	keywords = {nonparametric, ranks, t-test},
	pages = {1--1},
	file = {Full Text PDF:C\:\\Users\\karti\\Zotero\\storage\\SHCPEDCL\\McKnight and Najab - 2010 - Mann-Whitney U Test.pdf:application/pdf;Snapshot:C\:\\Users\\karti\\Zotero\\storage\\GN83Z4J5\\9780470479216.html:text/html}
